<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>edu.uta.googleappengine.chatapp.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>edu.uta.googleappengine.chatapp.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TokenServlet</servlet-name>
		<servlet-class>edu.uta.googleappengine.chatapp.TokenServlet</servlet-class>
	</servlet>
	<servlet>
       <servlet-name>FriendServlet</servlet-name>
       <servlet-class>edu.uta.googleappengine.chatapp.FriendServlet</servlet-class>
    </servlet>
    <servlet>
       <servlet-name>GetLoggedInUserServlet</servlet-name>
       <servlet-class>edu.uta.googleappengine.chatapp.GetLoggedInUserServlet</servlet-class>
    </servlet>
    <servlet>
       <servlet-name>MessageServlet</servlet-name>
       <servlet-class>edu.uta.googleappengine.chatapp.MessageServlet</servlet-class>
    </servlet>
    <servlet>
       <servlet-name>UploadServlet</servlet-name>
       <servlet-class>edu.uta.googleappengine.chatapp.UploadServlet</servlet-class>
    </servlet>
    <servlet>
       <servlet-name>ChatHistoryServlet</servlet-name>
       <servlet-class>edu.uta.googleappengine.chatapp.ChatHistoryServlet</servlet-class>
    </servlet>
    <servlet>
       <servlet-name>NumberOfLoginsPerUser</servlet-name>
       <servlet-class>edu.uta.googleappengine.chatapp.NumberOfLoginsPerUser</servlet-class>
    </servlet>
    <servlet>
       <servlet-name>NumOfLoginsPerHour</servlet-name>
       <servlet-class>edu.uta.googleappengine.chatapp.NumOfLoginsPerHour</servlet-class>
    </servlet>
	<servlet-mapping>
		<servlet-name>TokenServlet</servlet-name>
		<url-pattern>/gettoken</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
       <servlet-name>FriendServlet</servlet-name>
       <url-pattern>/getFriendList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
       <servlet-name>GetLoggedInUserServlet</servlet-name>
       <url-pattern>/getloggedinuser</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
       <servlet-name>MessageServlet</servlet-name>
       <url-pattern>/message/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
       <servlet-name>UploadServlet</servlet-name>
       <url-pattern>/uploadfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
       <servlet-name>ChatHistoryServlet</servlet-name>
       <url-pattern>/chathistory/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
       <servlet-name>NumberOfLoginsPerUser</servlet-name>
       <url-pattern>/getNoOfLoginsPerUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
       <servlet-name>NumOfLoginsPerHour</servlet-name>
       <url-pattern>/getNoOfLoginsPerHour</url-pattern>
    </servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
